{
  "name": "REV-Website",
  "version": "0.0.4",
  "description": "The website for Rensselaer Electric Vehicle",
  "license": "MIT",
  "authors": [{
    "name": "Ryan Delaney",
    "email": "delanr3@rpi.edu",
    "role": "Lead Developer"
  },{
    "name": "Andrew Marumoto",
    "email": "maruma@rpi.edu",
    "role": "Developer"
  }],
  "require": {
    "silex/silex": "1.2.1",
    "twig/twig": "1.16.0",
    "propel/propel": "~2.0@dev",
    "coffeescript/coffeescript": "dev-master",
    "jbinfo/mobile-detect-service-provider": "~1",
    "mthaml/silex-mthaml": "dev-master",
    "tedivm/jshrink": "~1.0",
    "natxet/CssMin": "dev-master",
    "richthegeek/phpsass": "dev-master",
    "igorw/config-service-provider": "1.2.*@dev",
    "jasig/phpcas": "1.3.*@dev",
    "symfony/twig-bridge": "~2.3",
    "aptoma/twig-markdown": "0.2.*",
    "michelf/php-markdown": "~1.3"
  },
  "suggest": {
      "intervention/image": "Allows image caching for the site"
  },
  "scripts": {
    "pre-update-cmd": "php -r \"@mkdir('app/propel/generated-classes');\"",
    "pre-install-cmd": "php -r \"@mkdir('app/propel/generated-classes');\"",
    "start": [
      "echo *** starting server on port 8000",
      "php -S localhost:8000 -t public_html"],
    "sql-build": "REVBuild\\SQL::build",
    "sql-deploy": "REVBuild\\SQL::deploy",
    "sql-diff": "REVBuild\\SQL::diff",
    "model-build": "REVBuild\\build::model",
    "config-build": "REVBuild\\build::config",
    "delete-cache": "REVBuild\\build::clearCache",
    "build": "REVBuild\\build::build",
    "deploy": "REVBuild\\deploy",
    "migrate": "REVBuild\\SQL::migrate"
  },
  "autoload": {
    "classmap": [
      "app/propel/generated-classes/"
    ],
    "psr-4": {
      "REVBuild\\": "scripts/"
    }
  },
  "config": {
    "process-timeout": 0
  }
}
